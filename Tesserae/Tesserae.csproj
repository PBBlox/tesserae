<Project Sdk="h5.Target/0.0.7793">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <DefineConstants>H5</DefineConstants>
    <AssemblyName>Tesserae</AssemblyName>
    <RootNamespace>Tesserae</RootNamespace>
    <LangVersion>7.2</LangVersion>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <DebugType>None</DebugType>
    <DebugSymbols>false</DebugSymbols>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="h5" Version="0.0.8001" />
    <PackageReference Include="h5.core" Version="0.0.8002" />
    <PackageReference Include="h5.Newtonsoft.Json" Version="0.0.7982" />
  </ItemGroup>

  <PropertyGroup>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Copyright>(c) Copyright 2020 Curiosity GmbH - all right reserved</Copyright>
    <Company>Curiosity GmbH</Company>
    <Authors>Curiosity GmbH</Authors>
    <PackageProjectUrl>https://github.com/curiosity-ai/tesserae</PackageProjectUrl>
    <PackageId>H5.Tesserae</PackageId>
    <Description>Tesserae is a UI toolkit for building web applications using H5</Description>
    <PackageTags>h5, HTML, UI</PackageTags>
    <NoWarn>1701;1702;1591</NoWarn>
  </PropertyGroup>
  
</Project>