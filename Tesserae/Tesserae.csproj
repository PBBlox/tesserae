<Project Sdk="h5.Target/0.0.11500">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <DefineConstants>H5</DefineConstants>
    <AssemblyName>tss</AssemblyName>
    <RootNamespace>Tesserae</RootNamespace>
    <LangVersion>7.2</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <DebugType>None</DebugType>
    <DebugSymbols>false</DebugSymbols>
    <SkipResourcesExtraction>true</SkipResourcesExtraction>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="h5" Version="0.0.11453" />
    <PackageReference Include="h5.core" Version="0.0.11312" />
    <PackageReference Include="h5.Newtonsoft.Json" Version="0.0.10147" />
  </ItemGroup>

  <PropertyGroup>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Copyright>(c) Copyright 2020 Curiosity GmbH - all right reserved</Copyright>
    <Company>Curiosity GmbH</Company>
    <Authors>Curiosity GmbH</Authors>
    <PackageProjectUrl>https://github.com/curiosity-ai/tesserae</PackageProjectUrl>
    <PackageId>Tesserae</PackageId>
    <Description>Tesserae is a UI toolkit for building web applications using the H5 C# to JavaScript compiler</Description>
    <PackageTags>h5, HTML, UI</PackageTags>
    <NoWarn>1701;1702;1591</NoWarn>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>
</Project>
